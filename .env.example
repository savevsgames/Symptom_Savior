# Supabase Configuration (Required)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Backend User Portal (Required for AI features)
# This should point to your backend server URL, NOT directly to the TxAgent container
EXPO_PUBLIC_BACKEND_USER_PORTAL=your_backend_portal_url_here

# Voice Services - ElevenLabs (Optional - for voice features)
EXPO_PUBLIC_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Video Avatar Services - TavusAI (Optional - future feature)
EXPO_PUBLIC_TAVUSAI_API_KEY=your_tavusai_api_key_here
EXPO_PUBLIC_TAVUSAI_REPLICA_ID=your_tavusai_replica_id_here

# Feature Toggles (true/false)
EXPO_PUBLIC_ENABLE_VOICE=true
EXPO_PUBLIC_ENABLE_EMERGENCY_DETECTION=true
EXPO_PUBLIC_ENABLE_VIDEO_AVATAR=false
EXPO_PUBLIC_DEBUG=true

# Emergency Services (Optional)
EXPO_PUBLIC_EMERGENCY_WEBHOOK_URL=your_emergency_webhook_url_here

# Development Settings
EXPO_NO_TELEMETRY=1